                        <TextBlock
                            Grid.Row="1"
                            Grid.Column="0"
                            VerticalAlignment="Center"
                            Classes="Margin8"
                            Text="{x:Static resx:ResUI.TbSettingsStartBoot}" />
                        <ToggleSwitch
                            x:Name="togAutoRun"
                            Grid.Row="1"
                            Grid.Column="1"
                            HorizontalAlignment="Left"
                            Classes="Margin8" />
                        <!--
						<TextBlock
                            Grid.Row="1"
                            Grid.Column="2"
                            VerticalAlignment="Center"
                            Classes="Margin8"
                            Text="{x:Static resx:ResUI.TbSettingsStartBootTip}"
                            TextWrapping="Wrap" />-->
