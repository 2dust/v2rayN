                        <TextBlock
                            Grid.Row="16"
                            Grid.Column="2"
                            VerticalAlignment="Center"
                            Classes="Margin8"
                            Text="{x:Static resx:ResUI.TbSettingsCurrentFontFamilyLinuxTip}"
